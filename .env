# This is not the variable to specify application environment. This variable is used in
# many npm package to run the packages in production mode. This variable is set to production 
# for all environments like dev, qa, prod. Pls don't change this value in any environment
NODE_ENV=production

# Variable to specificy application environment & port number
APP_ENVIRONMENT=development
PORT=80
# Environment varible for the debug npm package
DEBUG=fakeapis:*